{
    "name": "react-jssip-kit",
    "version": "1.0.1",
    "private": false,
    "description": "React hooks and provider around JsSIP client",
    "type": "module",
    "sideEffects": true,
    "types": "./dist/index.d.ts",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE",
        "CHANGELOG.md"
    ],
    "scripts": {
        "build": "tsup",
        "lint": "eslint \"src/**/*.{ts,tsx}\"",
        "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
        "format": "prettier --write \"{src,docs}/**/*.{ts,tsx,md,json}\"",
        "format:check": "prettier --check \"{src,docs}/**/*.{ts,tsx,md,json}\""
    },
    "keywords": [
        "react",
        "webrtc",
        "sip",
        "jssip",
        "hooks",
        "provider"
    ],
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vyacheslavshtyrlin/react-jssip.git"
    },
    "bugs": {
        "url": "https://github.com/vyacheslavshtyrlin/react-jssip/issues"
    },
    "homepage": "https://github.com/vyacheslavshtyrlin/react-jssip#readme",
    "author": "SIP Team",
    "peerDependencies": {
        "jssip": "^3.0.0",
        "react": ">=18 <20",
        "react-dom": ">=18 <20"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^8.25.0",
        "@typescript-eslint/parser": "^8.25.0",
        "eslint": "^9.21.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-react-hooks": "^5.1.0",
        "globals": "^15.14.0",
        "jssip": "^3.11.1",
        "prettier": "^3.5.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tsup": "^7.2.0",
        "typescript": "^5.0.0"
    }
}
